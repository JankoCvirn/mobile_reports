<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#000000"
    android:gravity="center_horizontal" >
            <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center"
                
                android:paddingTop="20dp" 
                
                >
                <TableRow
                    android:id="@+id/tableRow6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"  android:gravity="center" >
                    <ImageView android:src="@drawable/feclogo" />
                </TableRow>
                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="20dp" android:gravity="center"
                     >
                    <Button
                        android:id="@+id/btnPrefs"
                        style="@style/main_buttons4"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:text="User Preferences" />
                </TableRow>
                <TableRow
                    android:id="@+id/tableRow1a"
                    style="@style/headingBack"
                    android:layout_width="fill_parent"
                    android:layout_height="5dp"
                    android:gravity="center"
                    android:paddingTop="15dp" >
                     <Button
                         android:id="@+id/btnWork"
                         style="@style/main_buttons2"
                         android:layout_width="250dp"
                         android:layout_height="wrap_content"
                         android:paddingLeft="5dp"
                         android:text="Workers" />
                </TableRow>
                <TableRow
                    android:id="@+id/tableRow1b"
                    android:layout_width="fill_parent"
                    android:layout_height="5dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingTop="15dp" >
	                 <Button
	                     android:id="@+id/btnCust"
	                     style="@style/main_buttons2"
	                     android:layout_width="250dp"
	                     android:layout_height="wrap_content"
	                     android:text="Customers" />
                </TableRow>
				<TableRow
				    android:id="@+id/tableRow1c"
				    android:layout_width="fill_parent"
				    android:layout_height="5dp"
				    android:gravity="center"
				    android:orientation="vertical"
				    android:paddingTop="15dp" >
                     <Button
                         android:id="@+id/btnManager"
                         style="@style/main_buttons2"
                         android:layout_width="250dp"
                         android:layout_height="wrap_content"
                         android:paddingLeft="5dp"
                         android:text="Managers" />
                    
                </TableRow>
				<TableRow
				    android:id="@+id/tableRow1"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:paddingTop="15dp" android:gravity="center" >
					<Button
					    android:id="@+id/btnLocations"
					    style="@style/main_buttons2"
					    android:layout_width="250dp"
					    android:layout_height="wrap_content"
					    android:text="Locations" />
				</TableRow>
				<TableRow
				    android:id="@+id/tableRow14"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:paddingTop="15dp" android:gravity="center" >
					<Button
					    android:id="@+id/btnSubCont"
					    style="@style/main_buttons2"
					    android:layout_width="250dp"
					    android:layout_height="wrap_content"
					    android:text="SubContractor" />
				</TableRow>
				<TableRow
				    android:id="@+id/tableRow5"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:paddingTop="15dp" android:gravity="center">
					<Button
					    android:id="@+id/btnMate"
					    style="@style/main_buttons3"
					    android:layout_width="250dp"
					    android:layout_height="wrap_content"
					    android:text="Materials" />
				</TableRow>
                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_width="fill_parent"
                    android:layout_height="5dp"
                    android:gravity="center"
                    android:paddingTop="15dp" >
                    >

                 
                 
                 <Button
                     android:id="@+id/btnEqip"
                     style="@style/main_buttons3"
                     android:layout_width="250dp"
                     android:layout_height="wrap_content"
                     android:text="Eqiupment" />
                    
                </TableRow>
                <TableRow
                    android:id="@+id/tableRow1a"
                    style="@style/headingBack"
                    android:layout_width="fill_parent"
                    android:layout_height="5dp"
                    android:gravity="center"
                    android:paddingTop="15dp" >
                     <Button
                         android:id="@+id/btnUnits"
                         style="@style/main_buttons3"
                         android:layout_width="250dp"
                         android:layout_height="wrap_content"
                         android:text="Units" />
                    
                </TableRow>
                <TableRow
                    android:id="@+id/tableRow4"
                    android:layout_width="fill_parent"
                    android:layout_height="5dp"
                    android:gravity="center"
                    android:paddingTop="15dp" >
                    >
                <Button
                    android:id="@+id/btnForms"
                    style="@style/main_buttons4"
                    android:layout_width="200dp"
                    android:layout_height="wrap_content"
                    android:text="Reports" />
                </TableRow>
            </TableLayout>

            


</ScrollView>